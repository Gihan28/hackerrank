    const node1 = new SinglyLinkedListNode(10);
    const node2 = new SinglyLinkedListNode(20);
    const node3 = new SinglyLinkedListNode(30);
    const node4 = new SinglyLinkedListNode(40);
    const node5 = new SinglyLinkedListNode(50);
